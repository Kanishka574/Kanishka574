<h3>JavaScript</h3><p style="text-align: left;"><span style="color: #e65b00;">script.js</span></p><div style="background-color: #00002c; border-radius: 0.5rem; color: white; font-family: Consolas, &quot;Courier New&quot;, monospace; font-size: 14px; line-height: 19px; overflow: auto; padding: 0.3rem; white-space: pre;"><div style="line-height: 19px;"><div style="line-height: 19px;"><div style="line-height: 19px;"><div style="line-height: 19px;"><div><span style="color: #ff0062;">const</span> <span style="color: #eeffff;">toggleHeart</span> <span style="color: #ff0062;">=</span> <span style="color: #eeffff;">document</span>.<span style="color: cyan;">getElementById</span>('<span style="color: #eeff00;">heart</span>');</div><div>&nbsp; &nbsp; <span style="color: #eeffff;">toggleHeart</span>.<span style="color: cyan;">addEventListener</span>('<span style="color: #eeff00;">click</span>', () <span style="color: #ff0062;">=&gt;</span> {</div><div>&nbsp; &nbsp; &nbsp; &nbsp; <span style="color: #f07178;">toggleHeart</span>.<span style="color: #f07178;">classList</span>.<span style="color: cyan;">toggle</span>('<span style="color: #eeff00;">fa-solid</span>')</div><div>&nbsp; &nbsp; &nbsp; &nbsp; <span style="color: #ff006a;">if</span> (<span style="color: #f07178;">toggleHeart</span>.<span style="color: #f07178;">classList</span> <span style="color: #ff0062;">==</span> '<span style="color: #eeff00;">fa-regular fa-heart fa-solid</span>') {</div><div>&nbsp; &nbsp; &nbsp; &nbsp; &nbsp; &nbsp; <span style="color: #f07178;">toggleHeart</span>.<span style="color: #f07178;">style</span>.<span style="color: #f07178;">color</span> <span style="color: #ff0062;">=</span> '<span style="color: #eeff00;">red</span>'</div><div>&nbsp; &nbsp; &nbsp; &nbsp; }</div><div>&nbsp; &nbsp; &nbsp; &nbsp; <span style="color: #ff006a;">else</span> {</div><div>&nbsp; &nbsp; &nbsp; &nbsp; &nbsp; &nbsp; <span style="color: #f07178;">toggleHeart</span>.<span style="color: #f07178;">style</span>.<span styl